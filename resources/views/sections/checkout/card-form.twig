  <div
    class="flex h-12 gap-2 px-3 mt-2 border rounded-lg border-line group pending"
    x-init="$el.classList.remove('pending')">
    <div class="flex items-center justify-center w-8 shrink-0 [&_svg]:"
      x-html="icon()">

      <!-- SVG References -->
      <svg>
        <use xlink:href="#icon-default"></use>
      </svg>
    </div>

    <input type="text" placeholder="{{ p__('label', 'Card number') }}"
      class="flex items-center w-full p-0 text-base font-normal bg-transparent border-none rounded-none outline-hidden appearance-none focus:ring-0  transition-all group-[.pending]:opacity-0"
      data-pattern="{{ '{{9999}} {{9999}} {{9999}} {{9999}}' }}"
      :class="{ 'text-failure': errors.number }" x-ref="number"
      x-model="model.number">

    <input type="text" placeholder="{{ p__('label', 'MM/YY') }}"
      class="flex items-center p-0 text-base font-normal bg-transparent border-none rounded-none outline-hidden appearance-none w-14 focus:ring-0 transition-all group-[.pending]:opacity-0 group-[.pending]:translate-x-2"
      data-pattern="{{ '{{99}}/{{99}}' }}"
      :class="{ 'text-failure': errors.expiry }" x-ref="expiry"
      x-model="model.expiry">

    <input type="text" placeholder="CVC"
      class="flex items-center p-0 text-base font-normal bg-transparent border-none rounded-none outline-hidden appearance-none w-11 focus:ring-0 transition-all group-[.pending]:opacity-0 group-[.pending]:translate-x-4 delay-100"
      data-pattern="{{ '{{999}}' }}" :class="{ 'text-failure': errors.cvc }"
      x-ref="cvc" x-model="model.cvc">
  </div>

  <button type="submit" class="w-full mt-4 button button-accent" disabled
    :disabled="error || !data.number || !data.expire_month || !data.expire_year || !data.cvc">
    {{ p__('button', 'Subscribe') }}
  </button>

  <template x-if="error">
    <p
      class="flex items-center gap-1 mt-4 text-xs list-disc list-inside text-failure">
      <i class="text-base ti ti-square-rounded-x-filled"></i>
      <span x-text="error"></span>
    </p>
  </template>

  <template x-if="!error">
    <p
      class="flex items-center gap-1 mt-4 text-xs list-disc list-inside text-content-dimmed">
      <i class="text-base ti ti-lock"></i>
      {{ __('All card payments secured with :gateway.')|replace({':gateway' : card_gateway.name}) }}
    </p>
  </template>
