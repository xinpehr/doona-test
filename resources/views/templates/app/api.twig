{% extends "/layouts/base.twig" %}

{% block layout %}
<link rel="stylesheet"
  href="https://unpkg.com/swagger-ui-dist@5.11.0/swagger-ui.css" />
<div id="swagger-ui"></div>
<script src="https://unpkg.com/swagger-ui-dist@5.11.0/swagger-ui-bundle.js"
  crossorigin></script>
<script>
  window.onload = () => {
    window.ui = SwaggerUIBundle({
      url: `{{ specs }}`,
      dom_id: '#swagger-ui',
    });
  };
</script>
{% endblock %}